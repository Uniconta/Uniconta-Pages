<Pages:GridBasePage xmlns:Pages="clr-namespace:Uniconta.ClientTools.Page;assembly=ClientTools"
         xmlns:UC="clr-namespace:UnicontaClient.Pages;assembly=UnicontaPages"
x:Class="UnicontaClient.Pages.CustomPage.TMJournalLinePage"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:dx="http://schemas.devexpress.com/winfx/2008/xaml/core"
        xmlns:localPage="clr-namespace:UnicontaClient.Pages.CustomPage" 
        xmlns:dxg="http://schemas.devexpress.com/winfx/2008/xaml/grid" 
        xmlns:dxcn="http://schemas.devexpress.com/winfx/2008/xaml/core/internal"
        xmlns:docking="http://schemas.devexpress.com/winfx/2008/xaml/docking"
        xmlns:Util="clr-namespace:Uniconta.ClientTools.Util;assembly=ClientTools"
        xmlns:globalization="clr-namespace:System.Globalization;assembly=mscorlib"
        xmlns:dxe="http://schemas.devexpress.com/winfx/2008/xaml/editors"
        xmlns:utilityfunction="clr-namespace:UnicontaClient.Utilities"
        xmlns:dxednc="http://schemas.devexpress.com/winfx/2008/xaml/editors/internal"
        xmlns:CorasauControls="clr-namespace:Uniconta.ClientTools.Controls;assembly=ClientTools">
    <UserControl.Resources>
        <Util:IntToVisibilityConverter x:Key="IntToVisibilityConverter" />
        <Util:IntToColorConverter x:Key="IntToColorConverter" />
        <Util:IntToBoolConverter x:Key="IntToBoolConverter" />
        <Util:FiedlValueToForegroundColor x:Key="FiedlValueToForegroundColor"/>
        <Util:DoubleValConverter x:Key="DoubleValConverter"/>
        <Style TargetType="Run" x:Key="SummaryTotalStyle">
            <Setter Property="TextBlock.Foreground" Value="{Binding Column.TotalSummaries[2].Value, Converter={StaticResource FiedlValueToForegroundColor}}" />
        </Style>
        <Style x:Name="StyleRow" TargetType="dxg:RowControl">
            <Setter Property="Height" Value="Auto"/>
            <Style.Triggers>
                <DataTrigger Binding="{Binding Path=Row.IsMatched}" Value="True">
                    <Setter Property="Background" Value="LightBlue" />
                </DataTrigger>
            </Style.Triggers>
        </Style>
        <Style TargetType="{x:Type dxe:DateEdit}"
               x:Key="CustomColorDateEdit">
            <Style.Resources>
                <utilityfunction:DateEditColorConverter x:Key="DateEditColorConverter" />
                <Style TargetType="{x:Type dxednc:DateNavigatorCalendarCellButton}">
                    <Setter Property="Template">
                        <Setter.Value>
                            <ControlTemplate TargetType="{x:Type Button}">
                                <Grid x:Name="GridContent" Background="Transparent">
                                    <VisualStateManager.VisualStateGroups>
                                        <VisualStateGroup x:Name="CellStatesToday">
                                            <VisualState x:Name="CellStateToday">
                                                <Storyboard>
                                                    <ObjectAnimationUsingKeyFrames dxcn:ValueSetter.Visibility="Visible" Storyboard.TargetName="BorderToday" Storyboard.TargetProperty="Visibility" />
                                                    <ColorAnimation Duration="0" Storyboard.TargetName="Content" Storyboard.TargetProperty="ForegroundSolidColor4" To="#FFFFFFFF" />
                                                </Storyboard>
                                            </VisualState>
                                            <VisualState x:Name="CellStateNotToday" />
                                        </VisualStateGroup>
                                    </VisualStateManager.VisualStateGroups>
                                    <Border x:Name="MyBorderSpecial" CornerRadius="2">
                                        <Border.Background>
                                            <MultiBinding Converter="{StaticResource DateEditColorConverter}">
                                                <Binding RelativeSource="{RelativeSource AncestorType=dxednc:DateNavigatorCalendarCellButton}" Path="(dxednc:DateNavigatorCalendar.DateTime)"/>
                                                <Binding RelativeSource="{RelativeSource Self}" Path="(dxe:BaseEdit.OwnerEdit).MySpecialDates"/>
                                            </MultiBinding>
                                        </Border.Background>
                                    </Border>
                                    <Border x:Name="BorderToday" CornerRadius="2" Background="#FFFAD900" Visibility="Collapsed" />
                                    <dx:ContentContainer x:Name="Content" Content="{TemplateBinding Content}" Foreground="#FF444444" HorizontalAlignment="Center" Margin="1,0,1,1" VerticalAlignment="Center" />
                                </Grid>
                                <ControlTemplate.Triggers>
                                    <Trigger Property="dx:ThemeManager.IsTouchEnabled" Value="True">
                                        <Setter Property="MinWidth" TargetName="GridContent" Value="44" />
                                        <Setter Property="MinHeight" TargetName="GridContent" Value="44" />
                                    </Trigger>
                                </ControlTemplate.Triggers>
                            </ControlTemplate>
                        </Setter.Value>
                    </Setter>
                </Style>
            </Style.Resources>
        </Style>
    </UserControl.Resources>
    <Grid x:Name="LayoutRoot" Background="{StaticResource LayoutGridColor}">
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"/>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="10"></RowDefinition>
            <RowDefinition Height="*"></RowDefinition>
        </Grid.RowDefinitions>
        <CorasauControls:CorasauRibbonControl XMLSourcePath=";component/Pages/Project/TMJournalLinePageGridRibbon.xml" x:Name="localMenu"/>
        <Grid Grid.Row="1" Margin="5" x:Name="gridHeaderParam">
            <Grid.ColumnDefinitions>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="5"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="5"/>
                <ColumnDefinition Width="Auto"/>
                <ColumnDefinition Width="5"/>
                <ColumnDefinition Width="Auto"/>
            </Grid.ColumnDefinitions>
            <TextBlock Text="{Binding Converter={StaticResource GlobalLocalizationValueConverter},ConverterParameter=StartDate}" Grid.Column="0"  Margin="3,3,3,3"/>
            <CorasauControls:DateEditorEx Style="{StaticResource CustomColorDateEdit}" PopupClosed="txtDateTo_PopupClosed" x:Name="txtDateTo" Grid.Column="2"  Margin="0,3,3,3"/>
            <TextBlock Text="{Binding Converter={StaticResource GlobalLocalizationValueConverter},ConverterParameter=RegistrationType}" Visibility="Collapsed"  Grid.Column="4"  Margin="3,3,3,3"/>
            <CorasauControls:ComboBoxEditor x:Name="cmbRegistration" Visibility="Collapsed" IsReadOnly="True" IsEnabled="False"  Grid.Column="6"  Margin="0,3,3,3" />
        </Grid>
        <GridSplitter Grid.Row="2" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Background="{StaticResource WindowHeaderGradient}" />
        <CorasauControls:BusyIndicator x:Name="busyIndicator" Grid.Row="3" BusyContent="{Binding Converter={StaticResource GlobalLocalizationValueConverter},ConverterParameter=LoadingMsg}">
            <docking:DockLayoutManager x:Name="DockingControl" AllowCustomization="False">
                <docking:LayoutGroup x:Name="lGroup" AllowDrag="False" AllowDock="False" AllowFloat="False" Orientation="Vertical" AllowContextMenu="False" >
