                                                <CorasauControls:TextEditor Margin="30,4,0,0" IsPrintingMode="True" EditMode="InplaceInactive" EditValue="{Binding Path=DataContext.RowData.Row.Day1,Mode=OneWay,RelativeSource={RelativeSource TemplatedParent}, Converter={StaticResource DoubleValConverter}}" ValidateOnEnterKeyPressed="False" ValidateOnTextInput="False" DisplayFormatString="N2"  MaskUseAsDisplayFormat="False"/>
